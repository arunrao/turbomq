{
  "name": "turbomq",
  "version": "1.3.0",
  "description": "A powerful job queue system for Next.js applications",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist",
    "build:esm": "tsc -p tsconfig.json && find dist -name '*.js' -exec sh -c 'mv \"$1\" \"${1%.js}.mjs\"' _ {} \\;",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "prepare": "npm run build",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "migrate": "ts-node scripts/migrate-schema.ts"
  },
  "keywords": [
    "nextjs",
    "queue",
    "job-queue",
    "background-jobs",
    "prisma",
    "typescript",
    "serverless",
    "aws-lambda",
    "vercel",
    "scaling"
  ],
  "author": "Arun Rao",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "uuid": "^9.0.0"
  },
  "optionalDependencies": {
    "pg": "^8.11.0",
    "pg-pool": "^3.6.0"
  },
  "peerDependencies": {
    "next": ">=13.0.0",
    "react": ">=18.0.0",
    "socket.io": "^4.7.0",
    "socket.io-client": "^4.7.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^18.19.86",
    "@types/pg": "^8.11.14",
    "@types/react": "^18.0.0",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "axios": "^1.9.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "prisma": "^5.0.0",
    "socket.io": "^4.7.0",
    "socket.io-client": "^4.7.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "docs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/arunrao/turbomq.git"
  },
  "bugs": {
    "url": "https://github.com/arunrao/turbomq/issues"
  },
  "homepage": "https://github.com/arunrao/turbomq#readme"
}
