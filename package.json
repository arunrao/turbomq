{
  "name": "turbomq",
  "version": "1.4.0",
  "description": "A powerful job queue system for Next.js applications",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./queue": {
      "types": "./dist/queue.d.ts",
      "import": "./dist/queue.mjs",
      "require": "./dist/queue.js"
    },
    "./worker": {
      "types": "./dist/worker.d.ts",
      "import": "./dist/worker.mjs",
      "require": "./dist/worker.js"
    },
    "./worker-pool": {
      "types": "./dist/worker-pool.d.ts",
      "import": "./dist/worker-pool.mjs",
      "require": "./dist/worker-pool.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.mjs",
      "require": "./dist/types.js"
    },
    "./adapters/*": {
      "types": "./dist/adapters/*.d.ts",
      "import": "./dist/adapters/*.mjs",
      "require": "./dist/adapters/*.js"
    },
    "./testing/*": {
      "types": "./dist/testing/*.d.ts",
      "import": "./dist/testing/*.mjs",
      "require": "./dist/testing/*.js"
    }
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build:esm": "tsc -p tsconfig.json && find dist -name '*.js' -exec sh -c 'mv \"$1\" \"${1%.js}.mjs\"' _ {} \\; && node scripts/fix-imports.js",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "prepare": "npm run build",
    "test": "jest",
    "test:postgres": "cd test && npm run test:postgres",
    "lint": "eslint src --ext .ts,.tsx",
    "migrate": "ts-node scripts/migrate-schema.ts",
    "prepublishOnly": "npm run test && npm run lint"
  },
  "keywords": [
    "nextjs",
    "queue",
    "job-queue",
    "background-jobs",
    "prisma",
    "typescript",
    "serverless",
    "aws-lambda",
    "vercel",
    "scaling",
    "postgres",
    "sqlite"
  ],
  "author": "Arun Rao",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "cron-parser": "^5.1.1",
    "postgres": "^3.4.5",
    "uuid": "^9.0.0"
  },
  "optionalDependencies": {
    "pg": "^8.11.0",
    "pg-pool": "^3.6.0"
  },
  "peerDependencies": {
    "next": ">=13.0.0",
    "react": ">=18.0.0",
    "socket.io": "^4.7.0",
    "socket.io-client": "^4.7.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^18.19.86",
    "@types/pg": "^8.11.14",
    "@types/react": "^18.0.0",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "axios": "^1.9.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "prisma": "^5.0.0",
    "socket.io": "^4.7.0",
    "socket.io-client": "^4.7.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "docs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/arunrao/turbomq.git"
  },
  "bugs": {
    "url": "https://github.com/arunrao/turbomq/issues"
  },
  "homepage": "https://github.com/arunrao/turbomq#readme",
  "engines": {
    "node": ">=18.0.0"
  }
}
